 //write a c program to count the vowels and consonants in a string
#include<stdio.h>
#include<string.h>
int main()
{
  char s1[20];
  int c1=0,c2=0,i;
  printf("enter string");
  gets(s1);
  for(i=0;s1[i]!='\0';i++)
  {
    if(s1[i]=='a'||s1[i]=='A'||s1[i]=='e'||s1[i]=='E'||s1[i]=='i'||s1[i]=='I'||s1[i]=='o'||s1[i]=='O'||s1[i]=='u'||s1[i]=='U')
    c1++;
    else
    c2++;
    
  }
  printf("\n vowels count=%d",c1);
  printf("\n cousonants count=%d",c2);
}
